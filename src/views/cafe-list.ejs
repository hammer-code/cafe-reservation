<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Daftar Kafe | Cafe Reservation</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <div class="container">
    <h1>Daftar Kafe</h1>
    <a class="btn" href="/cafes/create">Tambah Baru</a>
    <hr>
    <ul>
      <% cafes.forEach(function (cafe) { %>
        <li>
          <a href="/cafes/<%= cafe.id %>"><%= cafe.name %></a>
        </li>
      <% }) %>
    </ul>
  </div>
</body>
</html>